}
function getFinal() {
    nlist.numcrunch();
    mean = sum / ncount;
    window.alert("The lowest number is " + least + "\nThe greastest number is " + greatest +
                 "\nThe mean is " + mean + "\nThe sum of all numbers is " + sum +
                 "\nThe product of all numbers is " + product);
}
function setText() {
