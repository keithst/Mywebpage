}
function Pali() {
    var findit = nlistP.start;
    var finditend = nlistP.end;
    var counter = 0;
    while (counter < strlng) {
        if (findit.letter == finditend.letter) {
            palid = true;
        }
        else {
            palid = false;
        }
        findit = findit.next;
        finditend = finditend.back;
        counter++;
    }
