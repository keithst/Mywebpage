@model WebApplication4.Models.BlogPost
@{
    ViewBag.Title = "Create";
}

<body class="bgcolor">
    <div class="bodytext boxshadow">
        <div class="headtext">
            <h1>Create Post</h1>
        </div>
    </div>
    <br />
    <form action="@Url.Action("Create", "Blog")" method="post">
        Title: <input type="text" name="Title" /><br />
        Body: <input type="text" name="Body" size="50"/><br />
        <button type="submit" class="btn btn-default btn-sm boxshadow"><span class="network-name shadow">Create</span></button>

    </form>
</body>
